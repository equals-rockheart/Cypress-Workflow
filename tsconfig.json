{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": ["ES2020", "DOM"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["cypress/e2e/pages/*"],
      "@support/*": ["cypress/support/*"],
      "@constants/*": ["cypress/support/constants/*"]
    },
    "types": ["cypress", "node", "./cypress/webpack-env.d.ts"]
  },
  "include": [
    "cypress.config.ts",
    "cypress/**/*.ts",
    "cypress/webpack-env.d.ts"
  ]
}
